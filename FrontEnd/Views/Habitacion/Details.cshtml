@model FrontEnd.Models.HabitacionViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ID
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Idhabitacion)
        </dd>
        <dt>
            Piso de la habitación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PisoHabitacion)
        </dd>
        <dt>
            Tipo de habitación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TipoHabitacion.NombreTipoHabitacion)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new {  id = Model.Idhabitacion }) |
    <a asp-action="Index">Regresar a la lista de habitaciones</a>
</div>
