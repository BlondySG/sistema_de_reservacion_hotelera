@model FrontEnd.Models.TipoHabitacionViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ID
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdtipoHabitacion)
        </dd>
        <dt>
            Tipo de habitación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NombreTipoHabitacion)
        </dd>
        <dt>
            Costo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CostoTipoHabitacion)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdtipoHabitacion }) |
    <a asp-action="Index">Regresar a lista de tipos de habitación</a>
</div>
